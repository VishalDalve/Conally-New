<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:id="@+id/card_view"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    card_view:cardCornerRadius="0dp"
    card_view:cardElevation="0dp"
    card_view:cardUseCompatPadding="false"
    card_view:cardBackgroundColor="@android:color/transparent">
<LinearLayout
    android:id="@+id/lay_row"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="@dimen/table_divider_height"
    android:background="@color/colorTableBack"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/textView2"
        style="@style/style_table_cell"
        android:layout_width="@dimen/width_col2"
        android:layout_height="@dimen/table_cell_height"/>

    <TextView
        android:id="@+id/textView3"
        style="@style/style_table_cell"
        android:layout_width="@dimen/width_col3"
        android:layout_height="@dimen/table_cell_height"/>

    <TextView
        android:id="@+id/textView4"
        style="@style/style_table_cell"
        android:layout_width="@dimen/width_col4"
        android:layout_height="@dimen/table_cell_height"/>

    <TextView
        android:id="@+id/textView5"
        style="@style/style_table_cell"
        android:layout_width="@dimen/width_col5"
        android:layout_height="@dimen/table_cell_height"/>

    <TextView
        android:id="@+id/textView6"
        style="@style/style_table_cell"
        android:layout_width="@dimen/width_col6"
        android:layout_height="@dimen/table_cell_height"/>

    <TextView
        android:id="@+id/textView7"
        style="@style/style_table_cell"
        android:layout_width="@dimen/width_col7"
        android:layout_height="@dimen/table_cell_height"/>

    <TextView
        android:id="@+id/textView8"
        style="@style/style_table_cell"
        android:layout_width="@dimen/width_col8"
        android:layout_height="@dimen/table_cell_height"/>
    <TextView
        android:id="@+id/textView9"
        style="@style/style_table_cell"
        android:layout_width="@dimen/width_col9"
        android:layout_height="@dimen/table_cell_height"/>

    <TextView
        android:id="@+id/textView10"
        style="@style/style_table_cell"
        android:layout_width="@dimen/width_col10"
        android:layout_height="@dimen/table_cell_height"/>

    <TextView
        android:id="@+id/textView11"
        style="@style/style_table_cell"
        android:layout_width="@dimen/width_col11"
        android:layout_height="@dimen/table_cell_height"/>

</LinearLayout>
</android.support.v7.widget.CardView>